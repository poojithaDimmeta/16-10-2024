#include<stdio.h>
#include<string.h>
void main()
{
    
    char a[10][10];
    char b;
    printf("enter the num of names");
    scanf("%d",&b);
    printf("enter the names");
    for(int i=0;i<b;i++)
    {
        scanf("%s",&a[i]);
    }
    printf("enter the legends");
    for(int i=0;i<b;i++)
    {
        printf("\n num %d.%s",i+1,a[i]);
    }
}
